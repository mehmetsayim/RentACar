@model RentACarData.Category


<div class="form-group">
    <label asp-for="MainMenuId"></label>
    <select asp-for="MainMenuId" asp-items="@ViewBag.MainMenus as SelectList" class="form-control">
        <option value="" disabled selected>Seçiniz...</option>
    </select>
    <span asp-validation-for="MainMenuId"></span>
</div>

<div class="form-group">
    <label asp-for="Name"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name"></span>
</div>
<div class="form-check">
    <input class="form-check-input" asp-for="Enabled">
    <label class="form-check-label" asp-for="Enabled"></label>
</div>
